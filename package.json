{
	"name": "frontend-heaven",
	"version": "1.0.0",
	"description": "Template for Single-page application based on React",
	"private": true,
	"scripts": {
		"start": "cross-env SERVE=true webpack serve --open --mode development",
		"start:dev": "webpack --mode development",
		"start:prod": "webpack --mode production",
		"lint": "npm run lint:scripts && npm run lint:styles",
		"lint:fix": "npm run lint:scripts:fix && npm run lint:styles:fix",
		"lint:styles": "stylelint **/*.scss",
		"lint:styles:fix": "stylelint **/*.scss --fix",
		"lint:scripts": "eslint src --ext .tsx",
		"lint:scripts:fix": "eslint src --ext .tsx --fix"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Romuelson/frontend-heaven.git"
	},
	"keywords": [
		"Frontend",
		"React",
		"Babel",
		"Webpack",
		"TypeScript",
		"Sass",
		"PostCSS",
		"ESLint",
		"Prettier",
		"Stylelint"
	],
	"author": "Romuelson",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Romuelson/frontend-heaven/issues"
	},
	"homepage": "https://github.com/Romuelson/frontend-heaven#readme",
	"devDependencies": {
		"@babel/code-frame": "^7.16.7",
		"@babel/core": "^7.17.2",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-react": "^7.16.7",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
		"@soda/friendly-errors-webpack-plugin": "^1.8.1",
		"@squoosh/lib": "^0.4.0",
		"@types/babel-code-frame": "^6.20.5",
		"@types/node": "^17.0.18",
		"@types/react": "^17.0.39",
		"@types/react-dom": "^17.0.11",
		"@types/webpack": "^5.28.0",
		"@typescript-eslint/eslint-plugin": "^5.12.0",
		"@typescript-eslint/parser": "^5.12.0",
		"babel-loader": "^8.2.3",
		"cross-env": "^7.0.3",
		"css-loader": "^6.6.0",
		"css-minimizer-webpack-plugin": "^3.4.1",
		"eslint": "^8.2.0",
		"eslint-config-airbnb": "^19.0.4",
