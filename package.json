	"dependencies": {
		"core-js": "^3.21.0",
		"dotenv": "^16.0.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"browserslist": {
		"production": [
			"> 0.2%",
			"not dead",
			"not op_mini all"
